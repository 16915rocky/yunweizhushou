<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
   xmlns:app ="http://schemas.android.com/apk/res-auto"     
    android:layout_width="match_parent"
    android:layout_height="50dp"
    android:layout_marginLeft="5dp"
    android:orientation="horizontal">
   <RelativeLayout 
        android:layout_width="50dp"
        android:layout_height="match_parent">
        <com.chinamobile.yunweizhushou.view.DashView
            android:id="@+id/dashView11"
	        android:layout_width="wrap_content"
	        android:layout_height="45dp"
	        app:dashWidth="2dp"
	        app:lineWidth="4dp"	    
	        app:lineColor="@color/color_blue"
	        app:dashOrientation="1"
	        android:layout_centerHorizontal="true"
	        app:lineHeight="1dp"/>     
	        <ImageView
               android:layout_width="8dp"
               android:layout_height="8dp"
           		android:layout_centerHorizontal="true"    
           		android:layout_alignParentBottom="true"                     
               android:background="@mipmap/icon_down_blue" />
	        <TextView 
	            android:id="@+id/tdfile"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:text="0R"
	            android:textSize="8sp"
	            android:layout_centerVertical="true"
	            android:layout_marginLeft="28dp"
	              android:textColor="@color/color_deepgreen"
	            />
    </RelativeLayout>
     <RelativeLayout 
         android:layout_width="60dp"
         android:layout_height="50dp">
         <com.chinamobile.yunweizhushou.view.DashView
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        app:dashWidth="2dp"
	        app:lineWidth="4dp"
	        android:layout_centerVertical="true"
	        app:lineColor="@color/color_blue"
	        app:dashOrientation="0"
	        app:lineHeight="1dp"/>
         <ImageView
            android:layout_width="8dp"
            android:layout_height="8dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:background="@mipmap/icon_left_blue" />
     </RelativeLayout>
	  <RelativeLayout
         android:layout_width="50dp"
         android:layout_height="50dp"                 
         android:background="@drawable/rectangle_gray" >
         <TextView
             android:id="@+id/xdlk"
             android:layout_height="wrap_content"
            android:layout_width="wrap_content"
             android:textSize="10sp"
             android:text="详单入库"            
              android:layout_centerHorizontal="true"
             android:layout_marginTop="15dp"
             android:textColor="@color/color_white"
           />
          <TextView
           
             android:layout_height="wrap_content"
            android:layout_width="wrap_content"
             android:textSize="10sp"
             android:layout_marginTop="28dp"
             android:text="0"
             android:layout_centerHorizontal="true"
             android:textColor="@color/color_white"
           />
          <com.chinamobile.yunweizhushou.view.WaveView
               android:id="@+id/waveview8"
              android:layout_alignParentBottom="true"
              android:layout_width="match_parent"              
              android:layout_height="0dp"   
                    
              />
     </RelativeLayout>
     <RelativeLayout 
        android:layout_width="30dp"
        android:layout_height="50dp">
           
     </RelativeLayout>
     <RelativeLayout
            android:layout_width="40dp"
            android:layout_height="50dp"
            android:gravity="center" >

            
			
           
                  
        </RelativeLayout>
        <LinearLayout 
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent">
            
        </LinearLayout>
        <RelativeLayout
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginRight="5dp"
            android:gravity="center"
            android:orientation="vertical" >
			<ImageView 
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:src="@mipmap/icon_shujukushili"/>
            
        </RelativeLayout>
</LinearLayout>
