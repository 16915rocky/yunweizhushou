<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
   xmlns:app ="http://schemas.android.com/apk/res-auto"     
    android:layout_width="match_parent"
    android:layout_height="50dp"
    android:layout_marginLeft="5dp"
    android:orientation="horizontal">
      <LinearLayout
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:orientation="vertical"
             android:background="@drawable/oval_gray"
             android:gravity="center" >

           
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textSize="10sp"
                android:text="经营分析"
                android:textColor="@color/color_white"
                />
          
           <TextView 
               android:id="@+id/fbacklog"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:text="0"
               android:textColor="@color/color_white"/>
        </LinearLayout>
     <RelativeLayout 
         android:layout_width="60dp"
         android:layout_height="50dp">
        
     </RelativeLayout>
	  <RelativeLayout
	         android:layout_width="50dp"
	         android:layout_height="50dp"
	         android:orientation="vertical">
          <com.chinamobile.yunweizhushou.view.DashView
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        app:dashWidth="2dp"
	        app:lineWidth="4dp"
	        android:layout_marginLeft="25dp"
	        app:lineColor="@color/color_blue"
	        app:dashOrientation="1"
	        app:lineHeight="1dp"/>      
	       
	     </RelativeLayout>
     <RelativeLayout 
        android:layout_width="30dp"
        android:layout_height="50dp">
         
     </RelativeLayout>
     <RelativeLayout
            android:layout_width="40dp"
            android:layout_height="50dp"
            android:gravity="center" >

           <ImageView
                android:layout_width="40dp"
                android:layout_height="50dp"
                android:src="@mipmap/icon_rhombus" />
			
           
         <LinearLayout 
             android:layout_width="match_parent"
             android:layout_height="wrap_content"
             android:orientation="vertical"
             android:gravity="center"
             android:layout_marginTop="16dp">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal" >

                    <ImageView
                        android:id="@+id/oval7_1"
                        android:layout_width="3dp"
                        android:layout_height="3dp"
                       android:src="@drawable/oval_deepgreen" />

                    <ImageView
                          android:id="@+id/oval7_2"
                        android:layout_width="3dp"
                        android:layout_height="3dp"
                        android:layout_marginLeft="2dp"
                        android:src="@drawable/oval_deepgreen" />

                    <ImageView
                          android:id="@+id/oval7_3"
                        android:layout_width="3dp"
                        android:layout_height="3dp"
                        android:layout_marginLeft="2dp"
                        android:src="@drawable/oval_deepgreen" />

                    <ImageView
                          android:id="@+id/oval7_4"
                        android:layout_width="3dp"
                        android:layout_height="3dp"
                        android:layout_marginLeft="2dp"
                        android:src="@drawable/oval_deepgreen" />
                </LinearLayout>
                 <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal" >

                    <ImageView
                        android:layout_width="3dp"
                        android:layout_height="3dp"
                        android:src="@drawable/oval_gray3" />

                    <ImageView
                        android:layout_width="3dp"
                        android:layout_height="3dp"
                        android:layout_marginLeft="2dp"
                        android:src="@drawable/oval_gray3" />

                    <ImageView
                        android:layout_width="3dp"
                        android:layout_height="3dp"
                        android:layout_marginLeft="2dp"
                        android:src="@drawable/oval_gray3" />

                    <ImageView
                        android:layout_width="3dp"
                        android:layout_height="3dp"
                        android:layout_marginLeft="2dp"
                        android:src="@drawable/oval_gray3" />
                </LinearLayout>
         </LinearLayout>
        </RelativeLayout>
        <LinearLayout 
            android:layout_width="20dp"
            android:layout_height="50dp">
          <RelativeLayout 
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        
	        <TextView 
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:text="路由\nMDB"
	            android:textSize="6sp"
	            android:layout_alignParentBottom="true"/>
	             
     </RelativeLayout>
                
        </LinearLayout>
        <LinearLayout 
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent">
            
        </LinearLayout>
         <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="5dp"
            android:gravity="center"
            android:orientation="vertical" >

            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:gravity="center"
                android:src="@mipmap/icon_ggprs_zhuji" />
          
        </LinearLayout>
</LinearLayout>
