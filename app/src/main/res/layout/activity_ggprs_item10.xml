<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
   xmlns:app ="http://schemas.android.com/apk/res-auto"     
    android:layout_width="match_parent"
    android:layout_height="50dp"
    android:layout_marginLeft="5dp"
    android:orientation="horizontal">
     <RelativeLayout
         android:layout_width="50dp"
         android:layout_height="50dp"                 
         android:background="@drawable/rectangle_gray" >
         <TextView
             android:id="@+id/hp"
             android:layout_height="wrap_content"
            android:layout_width="wrap_content"
             android:textSize="10sp"
             android:text="合并"            
              android:layout_centerHorizontal="true"
             android:layout_marginTop="15dp"
             android:textColor="@color/color_white"
           />
          <TextView
             android:id="@+id/ebacklog"
             android:layout_height="wrap_content"
            android:layout_width="wrap_content"
             android:textSize="10sp"
             android:layout_marginTop="28dp"
             android:text="0"
             android:layout_centerHorizontal="true"
             android:textColor="@color/color_white"
           />
          <com.chinamobile.yunweizhushou.view.WaveView
               android:id="@+id/waveview6"
              android:layout_alignParentBottom="true"
              android:layout_width="match_parent"              
              android:layout_height="0dp"   
                    
              />
     </RelativeLayout>
     <RelativeLayout 
         android:layout_width="60dp"
         android:layout_height="50dp">
         <com.chinamobile.yunweizhushou.view.DashView
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        app:dashWidth="2dp"
	        app:lineWidth="4dp"
	        android:layout_centerVertical="true"
	        app:lineColor="@color/color_blue"
	        app:dashOrientation="0"
	        app:lineHeight="1dp"/>
         <ImageView
            android:layout_width="8dp"
            android:layout_height="8dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:background="@mipmap/icon_left_blue" />
     </RelativeLayout>
	  <RelativeLayout
         android:layout_width="50dp"
         android:layout_height="50dp"                 
         android:background="@drawable/rectangle_gray" >
         <TextView
             android:id="@+id/hdff"
             android:layout_height="wrap_content"
            android:layout_width="wrap_content"
             android:textSize="10sp"
             android:text="话单分发"            
              android:layout_centerHorizontal="true"
             android:layout_marginTop="15dp"
             android:textColor="@color/color_white"
           />
          <TextView
             android:layout_height="wrap_content"
            android:layout_width="wrap_content"
             android:textSize="10sp"
             android:layout_marginTop="28dp"
             android:text="0"
             android:layout_centerHorizontal="true"
             android:textColor="@color/color_white"
           />
          <com.chinamobile.yunweizhushou.view.WaveView
               android:id="@+id/waveview7"
              android:layout_alignParentBottom="true"
              android:layout_width="match_parent"              
              android:layout_height="0dp"   
                    
              />
     </RelativeLayout>
     <RelativeLayout 
        android:layout_width="30dp"
        android:layout_height="50dp">
           
     </RelativeLayout>
     <RelativeLayout
            android:layout_width="40dp"
            android:layout_height="50dp"
            android:gravity="center" >

            
			
           
                  
        </RelativeLayout>
        <LinearLayout 
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent">
            
        </LinearLayout>
        <RelativeLayout
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginRight="5dp"
            android:gravity="center"
            android:orientation="vertical" >

         <View 
             android:layout_width="1dp"
             android:layout_height="match_parent"
             android:background="@color/color_blue"/>
            
        </RelativeLayout>
</LinearLayout>
