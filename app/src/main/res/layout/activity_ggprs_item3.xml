<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
   xmlns:app ="http://schemas.android.com/apk/res-auto"     
    android:layout_width="match_parent"
    android:layout_height="50dp"
    android:layout_marginLeft="5dp"
    android:orientation="horizontal">
     <RelativeLayout
         android:layout_width="50dp"
         android:layout_height="50dp"                 
         android:background="@drawable/rectangle_gray" >
         <TextView
             android:id="@+id/hdjm"
             android:layout_height="wrap_content"
            android:layout_width="wrap_content"
             android:textSize="10sp"
             android:text="话单解码"            
              android:layout_centerHorizontal="true"
             android:layout_marginTop="15dp"
             android:textColor="@color/color_white"
           />
          <TextView
              android:id="@+id/wbacklog"
             android:layout_height="wrap_content"
            android:layout_width="wrap_content"
             android:textSize="10sp"
             android:layout_marginTop="28dp"
             android:text="0"
             android:layout_centerHorizontal="true"
             android:textColor="@color/color_white"
           />
          <com.chinamobile.yunweizhushou.view.WaveView
              android:id="@+id/waveview1"
              android:layout_alignParentBottom="true"
              android:layout_width="match_parent"              
              android:layout_height="0dp"   
                    
              />
     </RelativeLayout>
     <RelativeLayout 
         android:layout_width="60dp"
         android:layout_height="50dp">
         <com.chinamobile.yunweizhushou.view.DashView
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        app:dashWidth="2dp"
	        app:lineWidth="4dp"
	        android:layout_centerVertical="true"
	        app:lineColor="@color/color_blue"
	        app:dashOrientation="0"
	        app:lineHeight="1dp"/>
         <ImageView
            android:layout_width="8dp"
            android:layout_height="8dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@mipmap/icon_right_blue" />
         <TextView 
             android:id="@+id/waflie"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:text="0R"
             android:textSize="8sp"
             android:layout_marginTop="10dp"
             android:textColor="@color/color_deepgreen"
             android:layout_centerHorizontal="true"/>
     </RelativeLayout>
	   <RelativeLayout
         android:layout_width="50dp"
         android:layout_height="50dp"                 
         android:background="@drawable/rectangle_gray" >
         <TextView
             android:id="@+id/fxcz"
             android:layout_height="wrap_content"
            android:layout_width="wrap_content"
             android:textSize="10sp"
             android:text="分析查重"            
              android:layout_centerHorizontal="true"
             android:layout_marginTop="15dp"
             android:textColor="@color/color_white"
           />
          <TextView
             android:id="@+id/abacklog"
             android:layout_height="wrap_content"
            android:layout_width="wrap_content"
             android:textSize="10sp"
             android:layout_marginTop="28dp"
             android:text="0"
             android:layout_centerHorizontal="true"
             android:textColor="@color/color_white"
           />
          <com.chinamobile.yunweizhushou.view.WaveView
               android:id="@+id/waveview2"
              android:layout_alignParentBottom="true"
              android:layout_width="match_parent"              
              android:layout_height="0dp"   
                    
              />
     </RelativeLayout>
     <RelativeLayout 
        android:layout_width="30dp"
        android:layout_height="50dp">
         <com.chinamobile.yunweizhushou.view.DashView
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        app:dashWidth="2dp"
	        app:lineWidth="4dp"
	        android:layout_centerVertical="true"
	        app:lineColor="@color/color_blue"
	        app:dashOrientation="0"
	        app:lineHeight="1dp"/>     
	        <com.chinamobile.yunweizhushou.view.DashView
	        android:layout_width="wrap_content"
	        android:layout_height="25dp"
	        android:layout_marginTop="25dp"
	        app:dashWidth="2dp"
	        app:lineWidth="4dp"
	       android:layout_marginLeft="15dp"
	        app:lineColor="@color/color_blue"
	        app:dashOrientation="1"
	        app:lineHeight="1dp"/>       
     </RelativeLayout>
     <RelativeLayout
            android:layout_width="40dp"
            android:layout_height="50dp"
            android:gravity="center" >

             <ImageView
                android:layout_width="40dp"
                android:layout_height="50dp"
                android:src="@mipmap/icon_rhombus" />
			
           
         <LinearLayout 
             android:layout_width="match_parent"
             android:layout_height="wrap_content"
             android:orientation="vertical"
             android:gravity="center"
             android:layout_marginTop="16dp">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal" >

                    <ImageView
                        android:id="@+id/oval4_1"
                        android:layout_width="3dp"
                        android:layout_height="3dp"
                        android:src="@drawable/oval_deepgreen" />

                    <ImageView
                          android:id="@+id/oval4_2"
                        android:layout_width="3dp"
                        android:layout_height="3dp"
                        android:layout_marginLeft="2dp"
                        android:src="@drawable/oval_deepgreen"  />

                    <ImageView
                          android:id="@+id/oval4_3"
                        android:layout_width="3dp"
                        android:layout_height="3dp"
                        android:layout_marginLeft="2dp"
                       android:src="@drawable/oval_deepgreen" />

                    <ImageView
                          android:id="@+id/oval4_4"
                        android:layout_width="3dp"
                        android:layout_height="3dp"
                        android:layout_marginLeft="2dp"
                        android:src="@drawable/oval_deepgreen" />
                </LinearLayout>
                 <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal" >

                    <ImageView
                        android:layout_width="3dp"
                        android:layout_height="3dp"
                        android:src="@drawable/oval_gray3" />

                    <ImageView
                        android:layout_width="3dp"
                        android:layout_height="3dp"
                        android:layout_marginLeft="2dp"
                        android:src="@drawable/oval_gray3" />

                    <ImageView
                        android:layout_width="3dp"
                        android:layout_height="3dp"
                        android:layout_marginLeft="2dp"
                        android:src="@drawable/oval_gray3" />

                    <ImageView
                        android:layout_width="3dp"
                        android:layout_height="3dp"
                        android:layout_marginLeft="2dp"
                        android:src="@drawable/oval_gray3" />
                </LinearLayout>
         </LinearLayout>
        </RelativeLayout>
        <LinearLayout 
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="50dp">
          <RelativeLayout 
        android:layout_width="match_parent"
        android:layout_height="match_parent">
         <com.chinamobile.yunweizhushou.view.DashView
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        app:dashWidth="2dp"
	        app:lineWidth="4dp"
	        android:layout_centerVertical="true"
	        app:lineColor="@color/color_blue"
	        app:dashOrientation="0"
	        app:lineHeight="1dp"/> 
	     <com.chinamobile.yunweizhushou.view.DashView
	        android:layout_width="match_parent"
	        android:layout_height="25dp"
	        app:dashWidth="2dp"
	        app:lineWidth="4dp"
	        android:layout_marginTop="25dp"
	        android:layout_marginLeft="15dp"
	        app:lineColor="@color/color_blue"
	        app:dashOrientation="1"
	        app:lineHeight="1dp"/>    
         <TextView 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="资料\nMDB"
            android:textSize="6sp"
            android:layout_alignParentBottom="true"/>
	             
     </RelativeLayout>
            
        </LinearLayout>
        <LinearLayout
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginRight="5dp"
            android:gravity="center"
            android:orientation="vertical" >

            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:gravity="center"
                android:src="@mipmap/icon_ggprs_zhuji" />
            
        </LinearLayout>
</LinearLayout>
